{
  "parser": "babel-eslint",
    "rules": {
      "camelcase": 1,
      "comma-dangle": [2],
      "indent": [2, 2, { "SwitchCase": 1 }],
      "linebreak-style": [2, "unix"],
      "max-len": [2, 100, 1, { "ignorePattern": "(^\\s*import.*from.*|.*http.*)", "ignoreUrls": true }],
      "no-cond-assign": [2],
      "no-console": [2],
      "no-constant-condition": [2],
      "no-control-regex": [2],
      "no-debugger": [2],
      "no-dupe-args": [2],
      "no-dupe-keys": [2],
      "no-duplicate-case": [2],
      "no-empty": [2],
      "no-empty-character-class": [2],
      "no-ex-assign": [2],
      "no-extra-boolean-cast": [2],
      "no-extra-parens": [0],
      "no-extra-semi": [2],
      "no-func-assign": [2],
      "no-inner-declarations": [2],
      "no-invalid-regexp": [2],
      "no-irregular-whitespace": [2],
      "no-negated-in-lhs": [2],
      "no-new": 0,
      "no-obj-calls": [2],
      "no-regex-spaces": [2],
      "no-sparse-arrays": [2],
      "no-underscore-dangle": 0,
      "no-unexpected-multiline": [2],
      "no-unreachable": [2],
      "no-var": 2,
      "object-shorthand": [2, "always"],
      "prefer-const": 1,
      "quotes": [2, "single"],
      "semi": [2, "always"],
      "use-isnan": [2],
      "valid-jsdoc": [2],
      "valid-typeof": [2],

      "accessor-pairs": [2],
      "block-scoped-var": [2],
      "complexity": [2],
      "consistent-return": [0],
      "curly": [2],
      "default-case": [2],
      "dot-notation": [2],
      "dot-location": [0],
      "eqeqeq": [2],
      "guard-for-in": [2],
      "no-alert": [0],
      "no-caller": [2],
      "no-div-regex": [2],
      "no-else-return": [0],
      "no-empty-label": [2],
      "no-eq-null": [2],
      "no-eval": [2],
      "no-extend-native": [2],
      "no-extra-bind": [2],
      "no-fallthrough": [2],
      "no-floating-decimal": [2],
      "no-implicit-coercion": [0],
      "no-implied-eval": [2],
      "no-invalid-this": [0],
      "no-iterator": [2],
      "no-labels": [2],
      "no-lone-blocks": [2],
      "no-loop-func": [2],
      "no-multi-spaces": [2],
      "no-multi-str": [2],
      "no-native-reassign": [2],
      "no-new-func": [2],
      "no-new-wrappers": [2],
      "no-new": [2],
      "no-octal-escape": [2],
      "no-octal": [2],
      "no-param-reassign": [0],
      "no-process-env": [0],
      "no-proto": [2],
      "no-redeclare": [2],
      "no-return-assign": [2],
      "no-script-url": [2],
      "no-self-compare": [2],
      "no-sequences": [2],
      "no-throw-literal": [2],
      "no-unused-expressions": [2],
      "no-unused-vars": [2],
      "no-use-before-define": [0],
      "no-useless-call": [2],
      "no-useless-concat": [2],
      "no-void": [2],
      "no-warning-comments": [2],
      "no-with": [2],
      "radix": [2],
      "vars-on-top": [2],
      "wrap-iife": [2],
      "yoda": [0],

      "array-bracket-spacing": [2],
      "block-spacing": [2],
      "brace-style": [0],
      "camelcase": [2],
      "comma-spacing": [2],
      "comma-style": [2],
      "computed-property-spacing": [2],
      "consistent-this": [2],
      "eol-last": [2],
      "func-names": [0],
      "func-style": [0],
      "id-length": [0],
      "id-match": [2],
      "key-spacing": [2],
      "lines-around-comment": [2],
      "linebreak-style": [2],
      "max-nested-callbacks": [2],
      "new-cap": [1],
      "new-parens": [2],
      "newline-after-var": [0],
      "no-array-constructor": [2],
      "no-continue": [2],
      "no-inline-comments": [2],
      "no-lonely-if": [2],
      "no-mixed-spaces-and-tabs": [2],
      "no-multiple-empty-lines": [2, { "max": 1 }],
      "no-nested-ternary": [2],
      "no-new-object": [2],
      "no-spaced-func": [2],
      "no-ternary": [0],
      "no-trailing-spaces": [2],
      "no-unneeded-ternary": [2],
      "object-curly-spacing": [2, "always"],
      "one-var": [2, "never"],
      "operator-assignment": [2],
      "operator-linebreak": [0],
      "padded-blocks": [0],
      "quote-props": [0],
      "require-jsdoc": [0],
      "semi-spacing": [2],
      "semi": [2],
      "sort-vars": [2],
      "space-after-keywords": [2],
      "space-before-blocks": [2],
      "space-before-function-paren": [2, "never"],
      "space-in-parens": [2],
      "space-infix-ops": [2],
      "space-return-throw-case": [2],
      "space-unary-ops": [2],
      "spaced-comment": [2],
      "wrap-regex": [2],

      "arrow-parens": [0],
      "arrow-spacing": [2],
      "constructor-super": [2],
      "generator-star-spacing": [2],
      "no-class-assign": [2],
      "no-const-assign": [2],
      "no-dupe-class-members": [2],
      "no-this-before-super": [2],
      "no-var": [2],
      "object-shorthand": [2],
      "prefer-arrow-callback": [0],
      "prefer-const": [2],
      "prefer-spread": [2],
      "prefer-reflect": [2],
      "prefer-template": [2],
      "require-yield": [2],

      "react/display-name": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 2,
      "react/jsx-curly-spacing": [2, "always"],
      "react/jsx-indent-props": [2, 2],
      "react/jsx-max-props-per-line": 0,
      "react/jsx-no-duplicate-props": 2,
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 2,
      "react/jsx-quotes": [2, "prefer-single"],
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/no-danger": 2,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 2,
      "react/no-multi-comp": 0,
      "react/no-set-state": 0,
      "react/no-unknown-property": 2,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 2,
      "react/require-extension": 2,
      "react/self-closing-comp": 2,
      "react/sort-comp": 0,
      "react/wrap-multilines": 2
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jasmine": true,
      "jquery": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "plugins": [
      "react"
    ],
    "globals": {
      "renderComponent": true
    }
}
